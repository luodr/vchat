<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.sinlo.vchat.mapper.DynamicMapper">
    <resultMap type="net.sinlo.vchat.entity.Dynamic" id="goodResultMap">
        <id column="id" property="id"/>
        <result column="send_user_id" property="send_user_id"/>
        <result column="type" property="type"/>
        <result column="context" property="context"/>
        <result column="images" property="images"/>
        <result column="name" property="name"/>
        <result column="img" property="img"/>
        <result column="remark" property="remark"/>
        <result column="createdAt" property="createdAt"/>
        <result column="updateAt" property="updateAt"/>
        <result column="deleteAt" property="deleteAt"/>
        <collection property="goods"  ofType="net.sinlo.vchat.entity.Good">
            <id column="g_user_id" property="user_id"/>
            <id column="g_id" property="id"/>
            <id column="g_updateAt" property="updateAt"/>
        </collection>
        <collection property="comments"  ofType="net.sinlo.vchat.entity.Comment">
            <id column="c_user_id" property="user_id"/>
            <id column="c_id" property="id"/>
            <id column="c_id" property="id"/>
            <id column="c_context" property="context"/>
        </collection>
    </resultMap>

</mapper>
